{
  "name": "basics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run zip",
    "gen": "idm-generate vue",
    "lint": "vue-cli-service lint",
    "zip": "idm zip"
  },
  "dependencies": {
    "@idm-modules/core": "^1.0.71",
    "ant-design-vue": "^1.7.6",
    "core-js": "^3.6.5",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@idm-modules/cli": "latest",
    "@idm-modules/cli-generate": "^1.0.33",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "svg-sprite-loader": "^4.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": 0,
      "no-empty": 0
    },
    "globals": {
      "IDM": true,
      "$": true,
      "_": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
